package testjava;


public class ArrayTest {
	int[] arr = {1,2,3,4};
	
	public static void main(String name[]) {
		printArrValue();
	}
	
	private void printArrValue() {
		ArrayTest atest = new ArrayTest();
		
		for (int i=0; i < atest.arr.length ; i++){
			System.out.print(atest.arr[i]);
				
		}
	}
	

}
