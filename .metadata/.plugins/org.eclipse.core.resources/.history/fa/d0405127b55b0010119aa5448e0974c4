package testjava;


public class ArrayTest {
	int[] arr = {1,2,3,4};
	
	public static void main(String name[]) {
		printArrValue();
	}
	
	static private void printArrValue() {
		ArrayTest atest = new ArrayTest();
		
		for int i=0; i < atest.arr.lenght ; i++{
			System.out.println(atest.arr[1]);
				
		}
	}
	

}
